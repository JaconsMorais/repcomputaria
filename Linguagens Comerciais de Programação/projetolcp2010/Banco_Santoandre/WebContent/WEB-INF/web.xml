<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>Banco_Santoandre</display-name>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>BancoLCP.Servlets.ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSacar</servlet-name>
        <servlet-class>BancoLCP.Servlets.ServletSacar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cadastrar</servlet-name>
        <servlet-class>BancoLCP.Servlets.ServletCadastrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEditar</servlet-name>
        <servlet-class>BancoLCP.Servlets.ServletEditar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletExcluir</servlet-name>
        <servlet-class>BancoLCP.Servlets.ServletExcluir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletDeposito</servlet-name>
        <servlet-class>BancoLCP.Servlets.ServletDeposito</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ValidaLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSacar</servlet-name>
        <url-pattern>/Sacar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cadastrar</servlet-name>
        <url-pattern>/ServletCadastrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEditar</servlet-name>
        <url-pattern>/Editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExcluir</servlet-name>
        <url-pattern>/Excluir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDeposito</servlet-name>
        <url-pattern>/Deposito</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
